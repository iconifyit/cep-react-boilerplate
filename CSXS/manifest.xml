<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest
        ExtensionBundleId="com.atomic.cep-barebones"
        ExtensionBundleVersion="1.0.0"
        Version="7.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.atomic.cep-barebones.panel" Version="1.0.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="ILST" Version="19.0" />
            <Host Name="DRWV" Version="15.0"/>
            <Host Name="FLPR" Version="15.0"/>
            <Host Name="IDSN" Version="11.0"/>
            <Host Name="AICY" Version="11.0"/>
            <Host Name="PHSP" Version="16.0"/>
            <Host Name="PHXS" Version="16.0"/>
            <Host Name="PPRO" Version="9.0"/>
            <Host Name="PRLD" Version="4.0"/>
            <Host Name="AEFT" Version="13.0"/>
            <Host Name="DEMO" Version="1.0"/>
            <Host Name="AUDT" Version="8.0"/>
            <Host Name="LTRM" Version="7.0"/>
            <Host Name="MUSE" Version="2015"/>
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="9.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.atomic.cep-barebones.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./client/index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>/
                    <Menu>CEP Barebones</Menu>
                    <Geometry>
                        <Size>
                            <Height>560</Height>
                            <Width>324</Width>
                        </Size>
                        <MaxSize>
                            <Height>560</Height>
                            <Width>324</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>560</Height>
                            <Width>324</Width>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icons/dog.png</Icon>
                        <Icon Type="RollOver">./icons/dog.png</Icon>
                        <Icon Type="DarkNormal">./icons/dog.png</Icon>
                        <Icon Type="DarkRollOver">./icons/dog.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
